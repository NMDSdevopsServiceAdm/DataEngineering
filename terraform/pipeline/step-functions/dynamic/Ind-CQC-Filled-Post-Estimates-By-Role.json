{
  "Comment": "A state machine indicating the transformation steps from Silver -> Gold data layers for the Independent CQC filled posts pipeline",
  "StartAt": "Silver to Gold Transformation Layer",
  "States": {
    "Silver to Gold Transformation Layer": {
      "Type": "Parallel",
      "Next": "Run validation crawler when success",
      "Branches": [
        {
          "StartAt": "Run estimates by job role",
          "States": {
            "Run estimates by job role": {
              "Type": "Parallel",
              "Branches": [
                {
                  "StartAt": "Estimate filled posts by job role (pyspark)",
                  "States": {
                    "Estimate filled posts by job role (pyspark)": {
                      "Type": "Task",
                      "Resource": "arn:aws:states:::glue:startJobRun.sync",
                      "Parameters": {
                        "JobName": "${estimate_ind_cqc_filled_posts_by_job_role_job}",
                        "Arguments": {
                          "--estimated_ind_cqc_filled_posts_source": "${dataset_bucket_uri}/domain=ind_cqc_filled_posts/dataset=ind_cqc_06_estimated_filled_posts/",
                          "--cleaned_ascwds_worker_source": "${dataset_bucket_uri}/domain=ASCWDS/dataset=worker_cleaned/",
                          "--estimated_ind_cqc_filled_posts_by_job_role_destination": "${dataset_bucket_uri}/domain=ind_cqc_filled_posts/dataset=ind_cqc_07_estimated_filled_posts_by_job_role/"
                        }
                      },
                      "End": true
                    }
                  }
                },
                {
                  "StartAt": "Estimate filled posts by job role (polars)",
                  "States": {
                    "Estimate filled posts by job role (polars)": {
                      "Type": "Task",
                      "Resource": "arn:aws:states:::ecs:runTask.sync",
                      "Parameters": {
                        "Cluster": "${model_cluster_arn}",
                        "TaskDefinition": "${independent_cqc_model_task_arn}",
                        "LaunchType": "FARGATE",
                        "NetworkConfiguration": {
                          "AwsvpcConfiguration": {
                            "Subnets": ${public_subnet_ids},
                            "SecurityGroups": ["${independent_cqc_model_security_group_id}"],
                            "AssignPublicIp": "ENABLED"
                          }
                        },
                        "Overrides": {
                          "ContainerOverrides": [
                            {
                              "Name": "_03_independent_cqc_estimate_filled_posts_by_job_role-container",
                              "Command": [
                                "estimate_ind_cqc_filled_posts_by_job_role.py",
                                "--estimates_source", "${dataset_bucket_uri}/domain=ind_cqc_filled_posts/dataset=ind_cqc_06_estimated_filled_posts/",
                                "--ascwds_job_role_counts_source", "${dataset_bucket_uri}/domain=ASCWDS/dataset=worker_cleaned/",
                                "--estimates_by_job_role_destination","${dataset_bucket_uri}/domain=ind_cqc_filled_posts/dataset=ind_cqc_07_estimated_filled_posts_by_job_role/"
                              ]
                            }
                          ]
                        }
                      },
                      "End": true
                    }
                  }
                }
              ]
            }
          }
        },
        {
          "StartAt": "Prepare ASCWDS Job Role Counts",
          "States": {
            "Prepare ASCWDS Job Role Counts": {
              "Type": "Task",
              "Resource": "arn:aws:states:::ecs:runTask.sync",
              "Parameters": {
                "Cluster": "arn:aws:ecs:eu-west-2:856699698263:cluster/1356-jr-pipeline-cluster",
                "TaskDefinition": "arn:aws:ecs:eu-west-2:856699698263:task-definition/1356-jr-pipeline-_03_independent_cqc-task:1",
                "LaunchType": "FARGATE",
                "NetworkConfiguration": {
                  "AwsvpcConfiguration": {
                    "Subnets": [
                      "subnet-0ec849a778438d382",
                      "subnet-025798c18c6ca7073",
                      "subnet-0e10939a615617030"
                    ],
                    "SecurityGroups": [
                      "sg-05633862db88c32b5"
                    ],
                    "AssignPublicIp": "ENABLED"
                  }
                },
                "Overrides": {
                  "ContainerOverrides": [
                    {
                      "Name": "_03_independent_cqc-container",
                      "Command": [
                        "prepare_job_role_counts.py",
                        "--cleaned_ascwds_worker_source",
                        "s3://sfc-1356-jr-pipeline-datasets/domain=ASCWDS/dataset=worker_cleaned/",
                        "--prepared_ascwds_job_role_counts_destination",
                        "s3://sfc-1356-jr-pipeline-datasets/domain=ind_cqc_filled_posts/dataset=ind_cqc_01_ascwds_prepared_job_role_counts/"
                      ]
                    }
                  ]
                }
              },
              "End": true
            }
          }
        }
      ],
      "Comment": "A state that fully encapsulates taking data from the Silver layer and performing ETL operations to move it to the Gold layer",
      "Catch": [
        {
          "ErrorEquals": [
            "States.ALL"
          ],
          "Next": "Publish error notification",
          "ResultPath": "$.error"
        }
      ]
    },
    "Run validation crawler when success": {
      "Type": "Task",
      "Parameters": {
        "Name": "1356-jr-pipeline-data_engineering_data_validation_reports"
      },
      "Resource": "arn:aws:states:::aws-sdk:glue:startCrawler",
      "Next": "Run Independent CQC crawler when success"
    },
    "Run Independent CQC crawler when success": {
      "Type": "Task",
      "Parameters": {
        "Name": "1356-jr-pipeline-data_engineering_ind_cqc_filled_posts"
      },
      "Resource": "arn:aws:states:::aws-sdk:glue:startCrawler",
      "Next": "Success"
    },
    "Publish error notification": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke.waitForTaskToken",
      "Parameters": {
        "FunctionName": "arn:aws:lambda:eu-west-2:856699698263:function:1356-jr-pipeline-job-failure-notification:$LATEST",
        "Payload": {
          "Error.$": "$.error.Cause",
          "ExecutionId.$": "$$.Execution.Id",
          "StateMachineName.$": "$$.StateMachine.Name",
          "StateMachineId.$": "$$.StateMachine.Id",
          "ExecutionStartTime.$": "$$.Execution.StartTime",
          "CallbackToken.$": "$$.Task.Token"
        }
      },
      "Next": "Run validation crawler when failed"
    },
    "Run validation crawler when failed": {
      "Type": "Task",
      "Parameters": {
        "Name": "1356-jr-pipeline-data_engineering_data_validation_reports"
      },
      "Resource": "arn:aws:states:::aws-sdk:glue:startCrawler",
      "Next": "Run Independent CQC crawler when failed"
    },
    "Run Independent CQC crawler when failed": {
      "Type": "Task",
      "Parameters": {
        "Name": "1356-jr-pipeline-data_engineering_ind_cqc_filled_posts"
      },
      "Resource": "arn:aws:states:::aws-sdk:glue:startCrawler",
      "Next": "Fail"
    },
    "Fail": {
      "Type": "Fail"
    },
    "Success": {
      "Type": "Succeed"
    }
  }
}
