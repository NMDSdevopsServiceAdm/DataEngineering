{
  "Comment": "A state machine for retraining Independent CQC models and generating predictions",
  "StartAt": "Model Retraining",
  "States": {
    "Model Retraining": {
      "Type": "Parallel",
      "Branches": [
        {
          "StartAt": "Build features - non-residential with dormancy",
          "States": {
            "Build features - non-residential with dormancy": {
              "Type": "Task",
              "Resource": "arn:aws:states:::ecs:runTask.sync",
              "Parameters": {
                "Cluster": "${model_cluster_arn}",
                "TaskDefinition": "${independent_cqc_model_task_arn}",
                "LaunchType": "FARGATE",
                "NetworkConfiguration": {
                  "AwsvpcConfiguration": {
                    "Subnets": ${independent_cqc_model_public_subnet_ids},
                    "SecurityGroups": ["${independent_cqc_model_security_group_id}"],
                    "AssignPublicIp": "ENABLED"
                  }
                },
                "Overrides": {
                  "ContainerOverrides": [
                    {
                      "Name": "_03_independent_cqc_model-container",
                      "Command": [
                        "model_01_features_non_res_with_dormancy.py",
                        "--bucket_name", "${dataset_bucket_name}",
                        "--model_name", "non_res_with_dormancy_model"
                      ]
                    }
                  ]
                }
              },
              "Next": "Train model and validate features - non-residential with dormancy"
            },
            "Train model and validate features - non-residential with dormancy": {
              "Type": "Parallel",
              "Branches": [
                {
                  "StartAt": "Train model - non-residential with dormancy",
                  "States": {
                    "Train model - non-residential with dormancy": {
                    "Type": "Task",
                    "Resource": "arn:aws:states:::ecs:runTask.sync",
                    "Parameters": {
                      "Cluster": "${model_cluster_arn}",
                      "TaskDefinition": "${independent_cqc_model_task_arn}",
                      "LaunchType": "FARGATE",
                      "NetworkConfiguration": {
                        "AwsvpcConfiguration": {
                          "Subnets": ${independent_cqc_model_public_subnet_ids},
                          "SecurityGroups": ["${independent_cqc_model_security_group_id}"],
                          "AssignPublicIp": "ENABLED"
                        }
                      },
                      "Overrides": {
                        "ContainerOverrides": [
                          {
                            "Name": "_03_independent_cqc_model-container",
                            "Command": [
                              "model_02_train.py",
                              "--bucket_name", "${dataset_bucket_name}",
                              "--model_name", "non_res_with_dormancy_model"
                            ]
                          }
                        ]
                      }
                    },
                    "Next": "Predict - non-residential with dormancy"
                  },
                  "Predict - non-residential with dormancy": {
                    "Type": "Task",
                    "Resource": "arn:aws:states:::ecs:runTask.sync",
                    "Parameters": {
                      "Cluster": "${model_cluster_arn}",
                      "TaskDefinition": "${independent_cqc_model_task_arn}",
                      "LaunchType": "FARGATE",
                      "NetworkConfiguration": {
                        "AwsvpcConfiguration": {
                          "Subnets": ${independent_cqc_model_public_subnet_ids},
                          "SecurityGroups": ["${independent_cqc_model_security_group_id}"],
                          "AssignPublicIp": "ENABLED"
                        }
                      },
                      "Overrides": {
                        "ContainerOverrides": [
                          {
                            "Name": "_03_independent_cqc_model-container",
                            "Command": [
                              "model_03_predict.py",
                              "--bucket_name", "${dataset_bucket_name}",
                              "--model_name", "non_res_with_dormancy_model"
                            ]
                          }
                        ]
                      }
                    },
                    "End": true
                    }
                  }
                },
                {
                  "StartAt": "Validate features - non-residential with dormancy",
                  "States": {
                    "Validate features - non-residential with dormancy": {
                      "Type": "Task",
                      "Resource": "arn:aws:states:::ecs:runTask.sync",
                      "Parameters": {
                        "Cluster": "${model_cluster_arn}",
                        "TaskDefinition": "${independent_cqc_model_task_arn}",
                        "LaunchType": "FARGATE",
                        "NetworkConfiguration": {
                          "AwsvpcConfiguration": {
                            "Subnets": ${independent_cqc_model_public_subnet_ids},
                            "SecurityGroups": ["${independent_cqc_model_security_group_id}"],
                            "AssignPublicIp": "ENABLED"
                          }
                        },
                        "Overrides": {
                          "ContainerOverrides": [
                            {
                              "Name": "_03_independent_cqc_model-container",
                              "Command": [
                                "validate_model_01_features.py",
                                "--bucket_name", "${dataset_bucket_name}",
                                "--source_path", "domain=ind_cqc_filled_posts/dataset=ind_cqc_04_features_non_res_with_dormancy_model/",
                                "--reports_path", "domain=data_validation_reports/dataset=data_validation_reports_ind_cqc_04_features_non_res_with_dormancy_model/",
                                "--compare_path", "domain=ind_cqc_filled_posts/dataset=ind_cqc_03_imputed_ascwds_and_pir/",
                                "--model", "non_res_with_dormancy_model"
                              ]
                            }
                          ]
                        }
                      },
                      "End": true
                    }
                  }
                }
              ],
              "End": true
            }
          }
        }
      ],
      "End": true
    }
  }
}
