                                        }
                                      },
                                      "Overrides": {
                                        "ContainerOverrides": [
                                          {
                                            "Name": "_03_independent_cqc_model-container",
                                            "Command": [
                                              "model_02_train.py",
                                              "--bucket_name", "${dataset_bucket_name}",
                                              "--model_name", "care_home_model"
                                            ]
                                          }
                                        ]
                                      }
                                    },
                                    "Next": "Predict CH"
