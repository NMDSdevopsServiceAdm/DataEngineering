                                "--source_path", "domain=ind_cqc_filled_posts/dataset=ind_cqc_01_merged_data/",
                                "--reports_path", "domain=data_validation_reports/dataset=data_validation_reports_ind_cqc_01_merged_ind_cqc_data/",
